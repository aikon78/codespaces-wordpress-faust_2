# WordPress site URL
# For Codespaces: use https://{codespace-name}-8080.app.github.dev
# For local development: use http://localhost:8080
# This will be auto-updated by setup scripts when using Codespaces
NEXT_PUBLIC_WORDPRESS_URL=http://localhost:8080

# Plugin secret key found in WordPress Settings->Headless
# Copy this from WordPress admin after installing and activating FaustWP plugin
FAUST_SECRET_KEY=your-secret-key-here

# The URL of your frontend site
# For Codespaces: use https://{codespace-name}-3000.app.github.dev
# For local development: use http://localhost:3000
# This will be auto-updated by setup scripts when using Codespaces
NEXT_PUBLIC_SITE_URL=http://localhost:3000
