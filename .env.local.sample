# Your WordPress site URL (will be set automatically in Codespaces)
NEXT_PUBLIC_WORDPRESS_URL=http://localhost:8080

# The URL of your site, used by Faust for sitemap generation
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# FaustWP Plugin secret (found in WordPress Admin > Headless > FaustWP)
# This must be copied from WordPress after installing the FaustWP plugin
FAUST_SECRET_KEY=your-secret-key-here
